{
  "name": "Udacity Playback Rate",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Update playback rate of Udacity videos.",
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/embed/*"],
      "js": ["playback.js"],
      "run_at": "document_idle",
      "all_frames": true
    },
    {
      "matches": ["*://www.udacity.com/view*"],
      "js": ["button.js"],
      "run_at": "document_end"
    }
  ]
}
